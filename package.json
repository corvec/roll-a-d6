{
  "name": "roll-a-d6",
  "version": "0.2.6",
  "description": "Sophisticated dice rolling library - tokenizer, parser, validator, and evaluator",
  "main": "./lib/index",
  "types": "./lib/roll-a-d6.d.ts",
  "keywords": [
    "dice",
    "D&D"
  ],
  "author": "Corey T Kump <Corey.Kump@gmail.com>",
  "license": "UNLICENSED",
  "dependencies": {},
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.6",
    "@babel/plugin-transform-modules-commonjs": "^7.9.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^25.4.0",
    "eslint": "^6.8.0",
    "eslint-plugin-fp": "^2.3.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsdoc": "^22.2.0",
    "jest": "^25.4.0",
    "jsdoc": "^3.6.4",
    "npm-watch": "^0.6.0",
    "tsd-jsdoc": "^2.5.0"
  },
  "watch": {
    "lint": {
      "pattern": ["lib"],
      "extensions": "mjs"
    },
    "test": {
      "pattern": ["lib"],
      "extensions": "mjs"
    }
  },
  "scripts": {
    "build-docs": "jsdoc -r lib/ -d jsdoc -R ./README.md -P ./package.json",
    "build-types": "jsdoc -r lib/ -t node_modules/tsd-jsdoc/dist -d types -R ./README.md -P ./package.json",
    "lint": "eslint **/*.mjs",
    "prep": "babel lib/*.mjs lib/**/*.mjs --ignore \"lib/**/*.test.mjs,lib/*.test.mjs\" --source-maps --out-dir lib",
    "test": "jest",
    "watch-lint": "npm-watch lint",
    "watch-test": "npm-watch test"
  },
  "babel": {
    "plugins": ["@babel/plugin-transform-modules-commonjs"]
  },
  "jest": {
    "moduleFileExtensions": ["mjs", "js"],
    "testMatch": ["**/**.test.mjs"],
    "transform": {
      "^.+\\.m?[jt]sx?$": "babel-jest"
    }
  }
}
